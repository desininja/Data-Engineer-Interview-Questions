    "lst = [1,2,3,4,5]\n",
    "\n",
    "# get odd and even numbers\n",
    "\n",
    "odd_lst = []\n",
    "even_lst = []\n",
    "\n",
    "for num in lst:\n",
    "    if num%2==0:\n",
    "        print(f\"{num} is Even\")\n",
    "        even_lst.append(num)\n",
    "    else:\n",
    "        print(f\"{num} is Odd\")\n",
    "        odd_lst.append(num)\n",
    "\n",
    "print(odd_lst)\n",
    "print(even_lst)"
